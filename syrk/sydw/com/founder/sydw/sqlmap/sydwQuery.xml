	<!-- 实有单位地图查询 -->
	<select id="queryDwDzOnPT" parameterClass="Map" resultClass="dwjbxxb">
		<![CDATA[SELECT * FROM (
					 SELECT ROWNUM R ,M.* FROM (
							SELECT A.ID, A.DWLBDM, A.DWMC,  P.ZBX, P.ZBY, A.DZ_DWDZXZ AS DWDZ,A.DZ_DWDZDM
  								FROM DW_DWJBXXB A,BZDZ_BZDZXXB_PT P
