		     </isEqual>
		    <isEqual property="dwjbxxb.type" compareValue="3" prepend="AND">
		     	 <![CDATA[SDE.ST_ENVINTERSECTS(p.SHAPE,]]>
		     	 <isNotNull property="dwjbxxb.zbz"><![CDATA[$dwjbxxb.zbz$]]></isNotNull>
		     	 <![CDATA[)=1]]>
		     </isEqual>		     
   		<![CDATA[) m  
		where rownum <= #end# ) 
			where r > #begin#  ]]>
	</select>
